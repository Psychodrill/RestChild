									<xs:annotation>
										<xs:documentation>Количество дополнительных мест</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:sequence>
								<xs:annotation>
									<xs:documentation>Компенсация за путевку</xs:documentation>
								</xs:annotation>
