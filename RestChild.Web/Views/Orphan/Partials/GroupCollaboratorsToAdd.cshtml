@model RestChild.Web.Models.Orphans.OrphanageCollaboratorsFilterModel

<div class="modal fade">
    <div class="modal-dialog big-dialog ">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">Сотрудники учреждения</h4>
            </div>
            <div class="modal-body modal-body-autosize" style="max-height: 600px">
                @using (Ajax.BeginForm("OrphanagePupilGroupListCollaboratorsChooseSearch", "Orphan", null, new AjaxOptions { HttpMethod = "POST", UpdateTargetId = "CollaboratorsToAdd", InsertionMode = InsertionMode.Replace }, new { @id = "CollaboratorsToAdd" }))
                {
                    @Html.Partial("Partials/GroupCollaboratorsToAddForm", Model)
                }
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Закрыть</button>
            </div>
        </div>
    </div>
</div>
